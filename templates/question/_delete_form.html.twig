<form method="post" action="{{ path('app_question_delete', {'id': questionSelected.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cette question ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ questionSelected.id) }}">
    <button class="btn w-100">Supprimer</button>
</form>
